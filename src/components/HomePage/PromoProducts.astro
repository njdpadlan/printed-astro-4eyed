---
const {promoProductsHeader, promoProductsImage, promoproductsText,  promoProductsBtn} = Astro.props;
const photoUrl = `${import.meta.env.STRAPI_URL}${promoProductsImage}`;
---

<div>
    <section>
        <h2>{promoProductsHeader}</h2>
        <p>{promoproductsText}</p>
        <img src={photoUrl} alt="random alt text">
        <a href="#">{promoProductsBtn}</a>
    </section>
</div>