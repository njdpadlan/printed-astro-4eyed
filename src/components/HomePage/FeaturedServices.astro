---
const {
    featuredServicesHeader, 
    clientBrandedTitle, 
    clientBrandedImage,
    clientBrandedAltText,
    warehouseTitle,
    warehouseImage,
    warehouseAltText,
    instantProofingTitle,
    instantProofingImage,
    instantProofingAltText
} = Astro.props;

---

<div>
    <h2>{featuredServicesHeader}</h2>
    <div>
        <section>
            <h3>{clientBrandedTitle}</h3>
            <img src={clientBrandedImage} alt={clientBrandedAltText}>
        </section>
        <section>
            <h3>{warehouseTitle}</h3>
            <img src={warehouseImage} alt={warehouseAltText}>
        </section>
        <section>
            <h3>{instantProofingTitle}</h3>
            <img src={instantProofingImage} alt={instantProofingAltText}>
        </section>
    </div>
</div>